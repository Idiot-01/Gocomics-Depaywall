{
    "manifest_version": 2,
    "name": "Gocomics Depaywall",
    "version": "2.1",
  
    "description": "Bypasses the Gocomics paywall and removes ads.",

    "homepage_url": "https://github.com/Idiot-01/Gocomics-Depaywall",
  
    "icons": {
      "48": "icons/border-48.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.gocomics.com/*"],
        "js": ["gocomics.js"]
      }
    ]
}
  